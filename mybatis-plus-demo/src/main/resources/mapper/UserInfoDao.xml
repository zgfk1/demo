    <!--查询单个-->
    <select id="queryById" resultMap="UserInfoMap" resultType="com.mybatisplus.demo.entity.UserInfo">
        select a.id,
               a.user_num,
               a.full_name,
               a.gender,
               a.nationality,
               a.id_card,
               a.birthday,
               a.political_status,
               a.in_party_time,
               a.telephone,
               a.employ_time_cop,
               a.employ_time_dept,
               a.major,
               a.team,
               a.professioanl_title,
               a.title_time,
               a.design_qualification,
               a.position_status,
               a.position_start_time,
               a.position_end_time,
               a.post_title,
               a.tutor,
               a.order_num,
               a.status,
               a.create_time,
               a.create_user,
               a.update_time,
               a.update_user,
               b.USER_ID,
               b.ROLE_ID,
               b.PROJECT_ID
        from office.user_info a
                 left join user_role b on a.id = b.USER_ID
        where id = #{id}
          and a.gender = '女'
    </select>

